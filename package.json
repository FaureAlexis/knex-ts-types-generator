{
  "name": "knex-types-generator",
  "version": "1.0.0",
  "description": "Generate Knex types from a database using schema introspection",
  "main": "dist/cli.js",
  "bin": {
    "knex-types-generator": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage"
  },
  "keywords": ["knex", "typescript", "types", "generator"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "knex": "^3.1.0",
    "ora": "^5.4.1",
    "pg": "^8.11.3",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^1.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  }
}
